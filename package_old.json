{
  "name": "skatepark.guide",
  "author": "Nico Martin - https://nico.dev",
  "license": "GPL-3.0+",
  "version": "1.0.0",
  "description": "A Progressive Web App for skateparks around the world",
  "scripts": {
    "dev": "npm run serve:dev & npm run build:dev",
    "prod": "npm run build:prod",
    "build:dev": "webpack -d --watch",
    "build:prod": "webpack -p",
    "serve:dev": "webpack-dev-server --open"
  },
  "dependencies": {
    "@types/google-map-react": "^1.1.8",
    "axios": "^0.21.1",
    "dayjs": "^1.10.4",
    "google-map-react": "^2.1.9",
    "idb": "^6.0.0",
    "preact": "^10.5.13",
    "react-hook-form": "^6.15.5",
    "react-intl": "^5.15.3",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/eslint-parser": "^7.12.1",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "favicons-webpack-plugin": "^4.2.0",
    "fs": "0.0.1-security",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.5",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "path": "^0.12.7",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.2.0",
    "postcss-mixins": "^6.2.3",
    "postcss-nested": "^5.0.3",
    "prettier": "^2.2.1",
    "prettier-stylelint": "^0.4.2",
    "pretty-quick": "^3.1.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-react-jsx",
        {
          "pragma": "h",
          "pragmaFrag": "Fragment"
        }
      ],
      "@babel/plugin-transform-runtime"
    ]
  },
  "browserslist": [
    "> 1%",
    "IE 11"
  ],
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
